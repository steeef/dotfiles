{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "upgrade_notice": false,
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "palette": {
    "rosewater": "#f4dbd6",
    "flamingo": "#f0c6c6",
    "pink": "#f5bde6",
    "mauve": "#c6a0f6",
    "red": "#ed8796",
    "maroon": "#ee99a0",
    "peach": "#f5a97f",
    "yellow": "#eed49f",
    "green": "#a6da95",
    "teal": "#8bd5ca",
    "sky": "#91d7e3",
    "sapphire": "#7dc4e4",
    "blue": "#8aadf4",
    "lavender": "#b7bdf8",
    "text": "#cad3f5",
    "subtext1": "#b8c0e0",
    "subtext0": "#a5adcb",
    "overlay2": "#939ab7",
    "overlay1": "#8087a2",
    "overlay0": "#6e738d",
    "surface2": "#5b6078",
    "surface1": "#494d64",
    "surface0": "#363a4f",
    "base": "#24273a",
    "mantle": "#1e2030",
    "crust": "#181926"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "powerline",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "background": "p:peach",
          "foreground": "p:crust"
        },
        {
          "type": "session",
          "style": "powerline",
          "template": "{{ if .SSHSession }}\ueba9 {{ .UserName }}@{{ .HostName }}{{ end }}",
          "background": "p:peach",
          "foreground": "p:crust"
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \uf07c  {{ .Path }} ",
          "properties": {
            "home_icon": "~",
            "style": "full"
          },
          "background": "p:surface2",
          "foreground": "p:text"
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:peach{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}p:teal{{ end }}",
            "{{ if gt .Behind 0 }}p:green{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": "\uf126 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "background": "p:sky",
          "foreground": "#000000"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} \ue718 ",
          "properties": {
            "fetch_version": true
          },
          "background": "#689f63",
          "foreground": "#ffffff"
        },
        {
          "type": "go",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue627 ",
          "properties": {
            "fetch_version": true
          },
          "background": "#00acd7",
          "foreground": "#111111"
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue235 ",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "background": "p:green",
          "foreground": "#000000"
        },
        {
          "type": "ruby",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue791 ",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "background": "#AE1401",
          "foreground": "#ffffff"
        },
        {
          "type": "kubectl",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "background_templates": [
            "{{if regexMatch \"prod\" .Context}}p:red{{end}}"
          ],
          "template": "\u2388 {{.Context}} ",
          "properties": {
            "display_default": false
          },
          "background": "p:sapphire",
          "foreground": "#000000"
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "template": " \uf0ad ",
          "background": "#ffff66",
          "foreground": "#111111"
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "template": " {{ .FormattedMs }} \uf252 ",
          "background": "p:yellow",
          "foreground": "#000000"
        },
        {
          "type": "status",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:surface0{{ end }}"
          ],
          "background_templates": [
            "{{ if gt .Code 0 }}p:red{{ end }}"
          ],
          "template": " {{ if gt .Code 0 }}{{ reason .Code }}{{ else }}\uf42e{{ end }} ",
          "properties": {
            "always_enabled": true
          },
          "background": "p:surface0",
          "foreground": "p:text"
        },
        {
          "type": "time",
          "style": "diamond",
          "invert_powerline": true,
          "template": " {{ .CurrentDate | date .Format }} \uf017 ",
          "background": "p:surface2",
          "foreground": "p:text"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "❯",
          "foreground": "p:green"
        }
      ],
      "newline": true
    }
  ],
  "transient_prompt": {
    "template": "❯ ",
    "background": "transparent",
    "foreground": "p:green"
  }
}
